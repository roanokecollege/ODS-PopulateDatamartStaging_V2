          SQLTask:SqlStatementSource="MERGE ODS_Staging.dbo.tempstudents as TARGET&#xA;USING (select hrper_id from dbo.ods_HRPER WHERE HRP_EFFECT_EMPLOY_DATE &lt; getdate() AND HRP_EFFECT_TERM_DATE is NULL) as SOURCE&#xA;&#x9;&#xA;ON TARGET.RCID = SOURCE.HRPER_ID&#xA;WHEN MATCHED THEN&#xA;&#x9;UPDATE SET RCEmployee = 'Y';&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
