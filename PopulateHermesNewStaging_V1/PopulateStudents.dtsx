      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2F90B10B-1B00-479F-98B9-8AE81E279EE7}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate DM_Student"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F7146AE4-AC88-4A9D-B361-F2DD18F42317}"
          SQLTask:SqlStatementSource="INSERT INTO HERMES.STAGING.dbo.DM_STUDENT&#xA;SELECT RCID, &#xA;&#x9;Username,&#xA;&#x9;Email,&#xA;&#x9; LastName, &#xA;&#x9; FirstName,&#xA;&#x9; MiddleName, &#xA;&#x9; FullName,&#xA;&#x9; Gender,&#x9;&#xA;&#x9; Age,&#xA;&#x9; Class,&#xA;&#x9; StartTerm,&#xA;&#x9; Box,&#x9;&#xA;&#x9;BoxCombination, -- added 2015 Jan KJohnson&#xA;&#x9; Building,&#xA;&#x9; LocalAddress1, &#xA;&#x9; LocalAddress2,&#xA;&#x9; LocalCity,&#xA;&#x9; LocalState,&#xA;&#x9; LocalZip,&#xA;&#x9; LocalPhone,&#xA;&#x9; HomeAddress1,&#xA;&#x9; HomeAddress2,&#xA;&#x9; HomeCity,&#xA;&#x9; HomeState,&#xA;&#x9; HomeZip,&#xA;&#x9; HomePhone,&#xA;&#x9; CellPhone,&#xA;&#x9; CurrentTerm,&#xA;&#x9; NextTerm,&#xA;&#x9; PreviousTerm,&#x9; &#xA;&#x9; ProgressReport,&#xA;&#x9; DirectoryCode,&#xA;&#x9; HS_CEEB, &#xA;&#x9; RCEmployee,&#xA;&#x9; AcadProgram,&#xA;&#x9; CURRENT_TIMESTAMP as ImportDate, &#xA;&#x9;DontSMS&#xA;&#xA;&#xA;&#xA;FROM ODS_STAGING.dbo.tempstudents&#xA;ORDER BY LastName, FirstName&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
    <DTS:Executable
      DTS:refId="Package\Populate ODS Staging Table TempStudents"
      DTS:CreationName="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{CA8FF23C-DFCA-4C50-AE48-8C93E1634980}"
      DTS:ExecutableType="Microsoft.SqlServer.Dts.Tasks.ExecuteSQLTask.ExecuteSQLTask, Microsoft.SqlServer.SQLTask, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91"
      DTS:LocaleID="-1"
      DTS:ObjectName="Populate ODS Staging Table TempStudents"
      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{F7146AE4-AC88-4A9D-B361-F2DD18F42317}"
          SQLTask:SqlStatementSource="DECLARE @currentTerm varchar(20);&#xA;&#xA;set @currentTerm = (SELECT dbo.FN_DATAMART_CURRENT_TERM());&#xA;&#xA;&#xA;&#xA;INSERT INTO ODS_Staging.dbo.tempstudents&#xA;SELECT DISTINCT term.RCID,&#xA;                          (SELECT     SUBSTRING(dbo.R_DATAMART_CAMPUS_EMAIL_VIEW.PREFERRED_EMAIL_ADDRESS, 1, CHARINDEX('@', &#xA;                                                   dbo.R_DATAMART_CAMPUS_EMAIL_VIEW.PREFERRED_EMAIL_ADDRESS, 1) - 1) AS Expr1) AS username, &#xA;                      dbo.R_DATAMART_CAMPUS_EMAIL_VIEW.PREFERRED_EMAIL_ADDRESS AS Email, RTRIM(dbo.R_PERSON.LAST_NAME) AS LastName, &#xA;                      RTRIM(dbo.R_PERSON.FIRST_NAME) AS FirstName, RTRIM(dbo.R_PERSON.MIDDLE_NAME) AS MiddleName, RTRIM(dbo.R_PERSON.FIRST_NAME) &#xA;                      + ' ' + RTRIM(COALESCE (dbo.R_PERSON.MIDDLE_NAME, '')) + ' ' + RTRIM(dbo.R_PERSON.LAST_NAME) AS FullName, &#xA;                      CASE GENDER WHEN 'M' THEN 'Male' WHEN 'F' THEN 'Female' ELSE NULL END AS GENDER,&#xA;                          (SELECT     dbo.FN_DATAMART_AGE(term.RCID) AS Expr1) AS AGE,&#xA;                          (SELECT     TOP (1) STA_CLASS&#xA;                            FROM          dbo.SPT_STUDENT_ACAD_LEVELS&#xA;                            WHERE      (STA_STUDENT = dbo.R_PERSON.ID)) AS CLASS, SPT_STUDENT_ACAD_LEVELS_1.STA_START_TERM AS StartTerm,&#xA;                          (SELECT DISTINCT TOP (1) ADDRESS_LINE1&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_1 = 'CM') OR&#xA;                                                   (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_2 = 'CM') OR&#xA;                                                   (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_3 = 'CM')) AS BOX,&#xA;                          (SELECT DISTINCT TOP (1) ADDR_MODIFIER_LINE&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_12&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_1 = 'CM') OR&#xA;                                                   (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_2 = 'CM') OR&#xA;                                                   (RCID = dbo.R_PERSON.ID) AND (ADDR_TYPE_3 = 'CM')) AS BoxCombination, &#xA;                      CASE rooms.BLDG_DESC WHEN 'Wells 1' THEN 'Wells' WHEN 'Wells 2' THEN 'Wells' WHEN 'Wells 3' THEN 'Wells' WHEN 'Yonce 4' THEN 'Yonce' WHEN 'Yonce 5'&#xA;                       THEN 'Yonce' ELSE rooms.BLDG_DESC END AS Building,&#xA;                          (SELECT     TOP (1) ADDRESS_LINE1&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_11&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_LOCAL_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS LocalAddress1,&#xA;                          (SELECT     TOP (1) ADDRESS_LINE2&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_10&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_LOCAL_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS LocalAddress2,&#xA;                          (SELECT     TOP (1) CITY&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_9&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_LOCAL_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS LocalCity,&#xA;                          (SELECT     TOP (1) STATE&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_8&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_LOCAL_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS LocalState,&#xA;                          (SELECT     TOP (1) DisplayZip&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_7&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_LOCAL_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS LocalZip,&#xA;                          (SELECT     TOP (1) NUMBER&#xA;                            FROM          dbo.R_DATAMART_PERSON_PHONES&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (NUMBERTYPE = 'LO')) AS LocalPhone,&#xA;                          (SELECT     TOP (1) ADDRESS_LINE1&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_6&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HomeAddress1,&#xA;                          (SELECT     TOP (1) ADDRESS_LINE2&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_5&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HomeAddress2,&#xA;                          (SELECT     TOP (1) CITY&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_4&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HomeCity,&#xA;                          (SELECT     TOP (1) STATE&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_3&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HomeState,&#xA;                          (SELECT     TOP (1) DisplayZip&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_2&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HomeZip,&#xA;                          (SELECT     TOP (1) ADDRESS_PHONES&#xA;                            FROM          dbo.R_DATAMART_PERSON_ADDRESSES_VIEW AS R_DATAMART_PERSON_ADDRESSES_VIEW_1&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (ADDRESS_ID =&#xA;                                                       (SELECT     dbo.FN_DATAMART_STUDENT_HOME_ADDRESS(dbo.R_PERSON.ID) AS Expr1))) AS HOMEPHONE,&#xA;                          (SELECT     TOP (1) NUMBER&#xA;                            FROM          dbo.R_DATAMART_PERSON_PHONES AS R_DATAMART_PERSON_PHONES_1&#xA;                            WHERE      (RCID = dbo.R_PERSON.ID) AND (NUMBERTYPE = 'CE')) AS CellPhone, term.CurrentTerm, term.NextTerm, term.PreviousTerm, 'N' AS ProgressReport, &#xA;                      CASE PRIVACY_FLAG WHEN 'N' THEN 'Y' WHEN 'D' THEN 'Y' WHEN 'SD' THEN 'Y' WHEN 'NP' THEN 'Y' WHEN 'NB' THEN 'Y' WHEN 'NU' THEN 'Y' WHEN 'NX' THEN&#xA;                       'Y' WHEN 'NZ' THEN 'Y' WHEN 'NR' THEN 'Y' WHEN 'NA' THEN 'Y' ELSE 'N' END AS DirectoryCode,&#xA;                          (SELECT     dbo.FN_DATAMART_LAST_ATTENDED_CEEB(dbo.R_PERSON.ID) AS Expr1) AS HS_CEEB, 'N' AS RCEmployee,&#xA;                          (SELECT     TOP (1) STPR_ACAD_PROGRAM&#xA;                            FROM          dbo.SPT_STUDENT_PROGRAMS AS Programs&#xA;                            WHERE      (STPR_ACAD_PROGRAM NOT LIKE 'ANT%') AND (STPR_CURRENT_STATUS = 'A') AND (STPR_STUDENT = dbo.R_PERSON.ID) AND (END_DATE IS NULL)) &#xA;                      AS acadprogram, dbo.R_STU_MISC2_L.STU_MISC2 AS DontSMS&#xA;FROM         ODS_Staging.dbo.tempstudentsterm AS term INNER JOIN&#xA;                      dbo.R_PERSON ON term.RCID = dbo.R_PERSON.ID LEFT OUTER JOIN&#xA;                      dbo.R_STU_MISC2_L ON dbo.R_PERSON.ID = dbo.R_STU_MISC2_L.STUDENTS_ID LEFT OUTER JOIN&#xA;                      dbo.ODS_HRPER ON dbo.R_PERSON.ID = dbo.ODS_HRPER.HRPER_ID LEFT OUTER JOIN&#xA;                      dbo.R_DATAMART_CAMPUS_EMAIL_VIEW ON dbo.R_PERSON.ID = dbo.R_DATAMART_CAMPUS_EMAIL_VIEW.ID LEFT OUTER JOIN&#xA;                      dbo.SPT_STUDENT_ACAD_LEVELS AS SPT_STUDENT_ACAD_LEVELS_1 ON dbo.R_PERSON.ID = SPT_STUDENT_ACAD_LEVELS_1.STA_STUDENT LEFT OUTER JOIN&#xA;                      dbo.R_DATAMART_ACTIVE_ROOM_ASSIGNMENTS_VIEW AS rooms ON term.RCID = rooms.RMAS_PERSON_ID AND rooms.RMAS_TERM = @currentTerm;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
