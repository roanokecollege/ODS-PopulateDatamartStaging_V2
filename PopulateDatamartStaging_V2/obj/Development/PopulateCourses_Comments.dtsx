        <SQLTask:SqlTaskData
          SQLTask:Connection="{8A0CAFEC-D040-4042-AC62-4197E1268721}"
          SQLTask:SqlStatementSource="MERGE ODS_Staging.dbo.courseComments as TARGET USING&#xA;&#x9;&#x9;&#x9;(select distinct COURSE_SECTIONS_ID, &#xA;&#x9;&#x9;&#x9;&#x9;dbo.FN_DATAMART_ACR_PRINTED_SPEC(R_ACAD_REQMTS.ACR_REQS_COURSE) as comment&#xA;&#x9;&#x9;&#x9;from ods_course_sections inner join ods_courses&#xA;&#x9;&#x9;&#x9;on ods_COURSE_SECTIONS.SEC_COURSE = ods_Courses.COURSES_ID INNER JOIN R_ACAD_REQMTS&#xA;&#x9;&#x9;&#x9;ON ODS_COURSES.COURSES_ID = R_ACAD_REQMTS.ACR_REQS_COURSE&#xA;&#x9;&#x9;&#x9;where NOT ACR_PRINTED_SPEC IS NULL) AS SOURCE&#xA;&#x9;ON TARGET.COURSEKEY = SOURCE.COURSE_SECTIONS_ID AND TARGET.COMMENT1 is NULL&#xA;&#x9;WHEN MATCHED THEN&#xA;&#x9;&#x9;UPDATE SET TARGET.COMMENT1 = SOURCE.comment;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
