          SQLTask:SqlStatementSource="INSERT INTO HERMES.Retention.dbo.veteran   (RCID, veteran)&#xA;SELECT  RCID,veteran&#xA; FROM dbo.View_Retention_Veteran&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.privacy_flags   (RCID, fkey_privacy_flag_id)&#xA;SELECT  RCID, fkey_privacy_flag_id &#xA; FROM    dbo.View_Retention_R_PERSON_FLAG&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.privacy_flag_types   (ID, description)&#xA;SELECT  ID, description &#xA; FROM    dbo.View_Retention_PRIVACY_CODES;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.term_notes  (STUDENT_TERM_NOTES_ID, rcid, fkey_term_id,sequence, created_at,note, fkey_term_note_type_id)&#xA;SELECT  STUDENT_TERM_NOTES_ID, rcid, fkey_term_id,sequence, created_at,note, fkey_term_note_type_id  &#xA; FROM    dbo.View_Retention_TERM_NOTES&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.term_note_types  (ID, description)&#xA;SELECT  ID, description  &#xA; FROM    dbo.View_Retention_TERM_TYPE_NOTES&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.student_perc_notifications  (fkey_rcid, sequence, start_date, end_date, fkey_perc_notification_id, portal_display)&#xA;SELECT fkey_rcid, sequence, start_date, end_date, fkey_perc_notification_id, portal_display    &#xA; FROM    dbo.student_perc_notifications&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.campus_organization_members  (fkey_rcid,fkey_campus_organization_id,fkey_status,start_date,end_date,role,role_status,role_start_date,role_end_date)&#xA;SELECT fkey_rcid,fkey_campus_organization_id,fkey_status,start_date,end_date,role,role_status,role_start_date,role_end_date&#xA; FROM  dbo.campus_organization_members&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.campus_organizations (id,fkey_group_id,fkey_org_type_id, description  )&#xA;SELECT id,fkey_group_id,fkey_org_type_id, description  &#xA; FROM  dbo.campus_organizations&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.perc_notifications (id,short_description,long_description,profile_display )&#xA;SELECT id,short_description,long_description,profile_display  &#xA; FROM  dbo.perc_notifications&#xA;;&#xA;&#xA;INSERT INTO HERMES.Retention.dbo.comments (RCID, comments)&#xA;Select RCID, comments&#xA; FROM  dbo.view_retention_comments&#xA;;&#xA;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
