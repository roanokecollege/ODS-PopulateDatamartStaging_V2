      DTS:TaskContact="Execute SQL Task; Microsoft Corporation; SQL Server 2012; © 2007 Microsoft Corporation; All Rights Reserved;http://www.microsoft.com/sql/support/default.asp;1"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{07ECD6CA-34CB-4F82-89EE-1D5F77873E68}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="INSERT INTO HERMES.STAGING.dbo.DM_REGISTRATIONS&#xA;SELECT     dbo.SPT_STUDENT_ACAD_CRED.STUDENT_ACAD_CRED_ID AS key_RegId, dbo.SPT_STUDENT_COURSE_SEC.SCS_COURSE_SECTION AS fkey_courseId, &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_PERSON_ID AS RCID, dbo.SPT_STUDENT_ACAD_CRED.STC_CURRENT_STATUS AS fkey_RegStatus, GETDATE() AS today, &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_GRADE AS gpa_value, dbo.SPT_STUDENT_ACAD_CRED.GRADE_VALUE AS grade, &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_CRED AS credits, dbo.R_GRADES.GRD_GRADE AS mid_term_grade, dbo.R_STUDENTS.X_MID_GPA AS term_gpa, &#xA;                      dbo.R_STUDENTS.X_STU_CUM_GPA AS cum_gpa, 'NA' AS permission, dbo.R_STU_MISC3_L.STU_MISC3 AS est_grad_date, &#xA;                      COALESCE (dbo.current_students_on_campus_view.LiveCampus, 0) AS live_campus&#xA;FROM         dbo.SPT_STUDENT_ACAD_CRED LEFT OUTER JOIN&#xA;                      dbo.current_students_on_campus_view ON &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_PERSON_ID = dbo.current_students_on_campus_view.RMAS_PERSON_ID LEFT OUTER JOIN&#xA;                      dbo.R_STU_MISC3_L RIGHT OUTER JOIN&#xA;                      dbo.R_STUDENTS ON dbo.R_STU_MISC3_L.STUDENTS_ID = dbo.R_STUDENTS.STUDENTS_ID ON &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_PERSON_ID = dbo.R_STUDENTS.STUDENTS_ID LEFT OUTER JOIN&#xA;                      dbo.R_STUDENT_COURSE_SEC INNER JOIN&#xA;                      dbo.R_GRADES ON dbo.R_STUDENT_COURSE_SEC.SCS_MID_TERM_GRADE1 = dbo.R_GRADES.GRADES_ID RIGHT OUTER JOIN&#xA;                      dbo.SPT_STUDENT_COURSE_SEC ON &#xA;                      dbo.R_STUDENT_COURSE_SEC.STUDENT_COURSE_SEC_ID = dbo.SPT_STUDENT_COURSE_SEC.STUDENT_COURSE_SEC_ID ON &#xA;                      dbo.SPT_STUDENT_ACAD_CRED.STC_STUDENT_COURSE_SEC = dbo.SPT_STUDENT_COURSE_SEC.STUDENT_COURSE_SEC_ID&#xA;WHERE     (dbo.SPT_STUDENT_ACAD_CRED.STC_CURRENT_STATUS &lt;&gt; 'C') AND (dbo.SPT_STUDENT_ACAD_CRED.STC_TERM IN&#xA;                          (SELECT     term&#xA;                            FROM          dbo.FN_DATAMART_GET_TERMS() AS FN_DATAMART_GET_TERMS_1))" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
