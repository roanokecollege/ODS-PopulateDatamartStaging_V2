      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{DEA24321-B3D9-4FEE-BF2E-7E16A5DF0BAF}"
          SQLTask:BypassPrepare="False"
          SQLTask:SqlStatementSource="INSERT INTO HERMES.STAGING.dbo.DM_Employees&#xA;&#xA;select DISTINCT ODS_Staging.dbo.facStaff.RCID, &#xA;&#x9;&#x9;FIRST_NAME as First,&#xA;&#x9;&#x9;LAST_NAME as Last,&#xA;&#x9; &#xA;&#x9;&#x9;COALESCE(COALESCE(PERSON_MAIL_LABEL, R_PERSON.PREFERRED_NAME), dbo.FN_DATAMART_FULL_NAME(ODS_Staging.dbo.facStaff.rcid)) AS FullName, &#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;(SELECT top 1 POSITION_TITLE FROM  dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID and primaryPosition = 1) &#xA;&#x9;&#x9;as Title,&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;(SELECT top 1 POSITION_TITLE FROM  dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID AND (primaryPosition is NULL or primaryPosition &lt;&gt; 1)&#xA;&#x9;&#x9;&#x9;order by pos) &#xA;&#x9;&#x9;&#x9; as Title2,&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;(SELECT top 1 POSITION_TITLE FROM  dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID AND (primaryPosition is NULL or primaryPosition &lt;&gt; 1)&#xA;&#x9;&#x9;&#x9;and POSITION_TITLE not in (SELECT top 1 POSITION_TITLE FROM  dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID AND (primaryPosition is NULL or primaryPosition &lt;&gt; 1)&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;order by pos)&#xA;&#x9;&#x9;&#x9;) &#xA;&#x9;&#x9;&#x9; as Title3,&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;(SELECT top 1 DEPARTMENT_DESC FROM  dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW &#xA;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID and primaryposition = 1)  &#xA;&#x9;&#x9; as Department,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;(SELECT SPT_BUILDINGS.BLDG_DESC&#xA;&#x9;&#x9;FROM SPT_BUILDINGS&#xA;&#x9;&#x9;WHERE SPT_BUILDINGS.BUILDINGS_ID = HRP_PRI_CAMPUS_BUILDING)  as Building,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;HRP_PRI_CAMPUS_OFFICE as Room,&#xA;&#x9;&#x9;HRP_PRI_CAMPUS_EXTENSION as Extension,&#xA;&#x9;&#x9;NULL as Extension1,&#xA;&#x9;&#x9;CampusEmail as Email,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;(select dbo.fn_DATAMART_SPOUSE_NAME (R_PERSON.ID)) as Spouse,&#xA;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;R_PERSON.NICKNAME as Nickname,&#xA;&#xA;&#x9;&#x9;COALESCE(COALESCE(PERSON_MAIL_LABEL, R_PERSON.PREFERRED_NAME), dbo.FN_DATAMART_FULL_NAME(ODS_Staging.dbo.facStaff.rcid)) AS Preferred_Name, &#xA;&#x9;&#x9;&#xA;&#x9;&#x9;PREFIX as Prefix,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;(SELECT SPOUSE.Nickname&#xA;&#x9;&#x9;FROM R_PERSON Spouse&#xA;&#x9;&#x9;WHERE Spouse.ID = R_PERSON.SPOUSE) as SpouseNickname,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;CASE (select COUNT(*) from dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;where HRPER_ID=ODS_Staging.dbo.facStaff.RCID and PERSTAT_STATUS = 'FTR' --and primaryposition = 1&#xA;&#x9;&#x9;&#x9;)&#xA;&#x9;&#x9;WHEN 0 then 'P'&#xA;&#x9;&#x9;ELSE 'F' &#xA;&#x9;&#x9;END as Status,&#xA;&#x9;&#xA;&#x9;&#x9;Username,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;CASE GENDER WHEN 'M' THEN 'Male' WHEN 'F' THEN 'Female' ELSE NULL END AS Gender, &#xA;&#x9;&#x9;address.ADDRESS_LINE1 as HomeAddress,&#xA;&#x9;&#x9;address.ADDRESS_LINE2 as HomeAddress2,&#xA;&#x9;&#x9;address.CITY as HomeCity,&#xA;&#x9;&#x9;address.STATE as HomeState,&#xA;&#x9;&#x9;address.ZIP as HomeZip,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;address.ADDRESS_PHONES as HomePhone,&#xA;        &#x9;&#xA;&#x9;&#x9;Faculty,&#xA;&#x9;&#x9;Staff,&#xA;&#x9;&#x9;Retiree,&#xA;&#x9;&#x9;case (select COUNT(*) from dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;where HRPER_ID=ODS_Staging.dbo.facStaff.RCID and POSITION_TITLE LIKE 'Chair%')&#xA;&#x9;&#x9;&#x9;WHEN 0 THEN 'N'&#xA;&#x9;&#x9;&#x9;ELSE 'Y'&#xA;&#x9;&#x9;END  as DeptChair,&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;(SELECT top 1 R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW.DEPARTMENT_DESC&#xA;&#x9;&#x9;&#x9;FROM dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW&#xA;&#x9;&#x9;&#x9;&#x9;WHERE HRPER_ID = ODS_Staging.dbo.facStaff.RCID and primaryPosition = 1 and deptSec = 1&#xA;&#x9;&#x9;&#x9;&#x9;and substring(perpos_position_id,1,5) &lt;&gt; '25011' AND substring(perpos_position_id, 1,5) &lt;&gt;'30030') as&#xA;&#x9;&#x9;&#x9;  Sec1Dept,&#xA;&#xA;&#xA;--- secretary for multiple depts currently not stored in the data in a way that it can be derived&#xA;--hardcoded.&#xA;&#x9;&#x9;&#x9; CASE ODS_Staging.dbo.facStaff.RCID&#xA;&#x9;&#x9;&#x9; WHEN '0042195' THEN 'Religion/Philosophy'&#xA;&#x9;&#x9;&#x9; WHEN '0085395' THEN 'Business Admin. &amp; Economics'&#xA;&#x9;&#x9;&#x9; WHEN '0957897' THEN 'Health and Human Performance'&#xA;&#x9;&#x9;&#x9; WHEN '0224154' THEN 'Psychology'&#xA;&#x9;&#x9;&#x9; WHEN '0048527' THEN 'Environmental Studies'&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9; END as Sec2Dept,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;(select TOP 1 R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW.DEPARTMENT_DESC&#xA;&#x9;&#x9; from dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW &#xA;&#x9;&#x9;&#x9;where HRPER_ID=ODS_Staging.dbo.facStaff.RCID&#xA;&#x9;&#x9;AND POSITION_CLASS = 'FAC' &#xA;&#x9;&#x9;AND PERPOS_DEPT &lt;&gt; (SELECT top 1 PERPOS_DEPT FROM dbo.R_DATAMART_EMPLOYEE_CURRENT_POSITIONS_VIEW &#xA;&#x9;&#x9;WHERE HRPER_ID = R_PERSON.ID and primaryposition=1) &#xA;&#x9;&#x9;)  as AdditionalDepartment,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;case PRIVACY_FLAG&#xA;&#x9;&#x9;WHEN 'N' THEN 'Y'&#xA;&#x9;&#x9;ELSE 'N'&#xA;&#x9;&#x9;END AS ExcludeDirectory,&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;case DIRECTORY_FLAG&#xA;&#x9;&#x9;WHEN 'NA' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NB' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NU' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NY' THEN 'Y'&#xA;&#x9;&#x9; WHEN 'NC' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NE' THEN 'Y'        &#xA;        &#x9;&#x9;WHEN 'NF' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NG' THEN 'Y'&#xA;&#x9;&#x9;ELSE 'N'&#xA;&#x9;&#x9;END AS ExcludeAddress,&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;case DIRECTORY_FLAG&#xA;&#x9;&#x9;WHEN 'NP' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NB' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NU' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NZ' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'ND' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NE' THEN 'Y'    &#xA;        &#x9;&#x9;WHEN 'NF' THEN 'Y'    &#xA;        &#x9;&#x9;WHEN 'NG' THEN 'Y'    &#xA;        &#x9;&#x9;WHEN 'NH' THEN 'Y'    &#xA;        &#x9;&#x9;WHEN 'NI' THEN 'Y'    &#xA;&#x9;&#x9;ELSE 'N'&#xA;&#x9;&#x9;END AS ExcludePhone,&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;case DIRECTORY_FLAG&#xA;&#x9;&#x9;WHEN 'NS' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NC' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'ND' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NE' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NF' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NG' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NH' THEN 'Y'&#xA;        &#x9;&#x9;WHEN 'NI' THEN 'Y'&#xA;&#x9;&#x9;ELSE 'N'&#xA;&#x9;&#x9;END AS ExcludeSpouse,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;case DIRECTORY_FLAG&#xA;&#x9;&#x9;WHEN 'NU' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NX' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NY' THEN 'Y'&#xA;&#x9;&#x9;WHEN 'NZ' THEN 'Y'       &#xA;        &#x9;&#x9;WHEN 'NF' THEN 'Y'    &#xA;        &#x9;&#x9;WHEN 'NH' THEN 'Y'    &#xA;&#x9;&#x9;ELSE 'N'&#xA;&#x9;&#x9;END AS ExcludePhoto,&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;GETDATE() as ImportDate,&#xA;&#xA;(select top 1 PositionClass from view_HRPER where view_HRPER.RCID= R_PERSON.ID) as EmployeeClass&#xA;&#xA;&#x9;&#x9;FROM ODS_Staging.dbo.facStaff INNER JOIN R_PERSON&#xA;&#x9;&#x9;ON ODS_Staging.dbo.facStaff.RCID = R_PERSON.ID LEFT OUTER JOIN dbo.R_DATAMART_CAMPUS_EMAIL_VIEW&#xA;&#x9;&#x9;on ODS_Staging.dbo.facStaff.RCID = dbo.R_DATAMART_CAMPUS_EMAIL_VIEW.ID &#xA;&#x9;&#x9;LEFT OUTER JOIN ODS_HRPER on ODS_Staging.dbo.facStaff.RCID = ODS_HRPER.HRPER_ID  &#xA;&#x9;&#x9;LEFT OUTER JOIN dbo.R_DATAMART_PERSON_ADDRESSES_VIEW address&#xA;&#x9;&#x9;on R_Person.PREFERRED_ADDRESS = address.ADDRESS_ID and r_person.id = address.rcid&#xA;&#x9;&#x9;LEFT OUTER JOIN R_PER_MAIL_LABEL_L ON R_PERSON.ID = R_PER_MAIL_LABEL_L.ID AND R_PER_MAIL_LABEL_L.POS = 1" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
