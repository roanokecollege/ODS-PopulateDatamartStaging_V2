      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{DEA24321-B3D9-4FEE-BF2E-7E16A5DF0BAF}"
          SQLTask:SqlStatementSource="MERGE INTO ODS_Staging.dbo.facStaff as TARGET&#xA;USING&#x9;(SELECT ID, SUBSTRING(PREFERRED_EMAIL_ADDRESS, 1, CHARINDEX('@',PREFERRED_EMAIL_ADDRESS, 1) - 1), PREFERRED_EMAIL_ADDRESS&#xA;&#x9;&#x9; FROM dbo.R_DATAMART_CAMPUS_EMAIL_VIEW) AS SOURCE(ID,Username, EmailAddress) ON TARGET.RCID = SOURCE.ID&#xA;WHEN MATCHED THEN&#xA;&#x9;UPDATE SET TARGET.Username = SOURCE.username, TARGET.CampusEmail = SOURCE.EmailAddress;&#xA;&#xA;--clean up emails to account for DIR type vs campus email&#xA;MERGE ODS_Staging.dbo.facStaff as TARGET&#xA;USING (SELECT ID, R_PER_PEOPLE_EMAIL.PERSON_EMAIL_ADDRESSES&#xA;&#x9;&#x9;FROM R_PER_PEOPLE_EMAIL INNER JOIN ODS_Staging.dbo.facStaff&#xA;&#x9;&#x9;ON id = rcid&#xA;&#x9;&#x9;WHERE R_PER_PEOPLE_EMAIL.PERSON_EMAIL_TYPES = 'DIR'&#xA;&#x9;&#x9;and PERSON_EMAIL_ADDRESSES &lt;&gt; CampusEmail) AS SOURCE (ID, EmailAddress) on (TARGET.RCID = SOURCE.ID)&#xA;WHEN MATCHED THEN&#xA;&#x9;UPDATE set CampusEmail = SOURCE.EmailAddress;&#xA;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
